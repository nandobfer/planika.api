datasource db {
	provider = "mysql"
	url = env("DATABASE_URL")
}
generator client {
	provider = "prisma-client-js"
	previewFeatures = ["fullTextSearch","fullTextIndex"]
}

model User {
    id                  String    @id
    name                String
    email               String    @unique
    password            String
    createdAt           String
    defaultCurrency     String  @default("BRL")
    picture             String?
}

model CurrencyRate {
    id          Int     @id @default(autoincrement())
    symbol      String
    name        String
    code        String  @unique
    updatedAt   String
    base        String
    rate        Float
}
